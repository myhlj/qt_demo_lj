#comboBox_chanel
{
    background-color: #0f2b46;
    border: 1px solid white;
    font-size: 22px;
    color: #ffffff;
    selection-background-color: #2aa1d3;
}


#comboBox_chanel::down-arrow
{
    background-image: url(:/img/page/select-right.png);
    min-width: 14px;

    background-position: center left;
    background-repeat: no-repeat;
}

#comboBox_chanel::down-arrow:pressed
{
    image: url(:/img/page/select-up.png);
    min-width: 14px;
    background-position: center left;
    background-repeat: no-repeat;
}


#comboBox_chanel::drop-down
{
    background-color: #0f2b46;
}

